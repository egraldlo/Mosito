  						//yyerror("First statement discarded, input new statement"); 
  						yyclearin;
  						yyerrok; 				
  					}	// 2014-3-4---增加对错误的支持---by余楷
  	| stmt_list error ';'	{ 
  						printf(">> \n"); 
  						parsetreeroot = NULL;	// 2014-3-6---解析错误后置根结点为空---by余楷
  						errorNumber++;		// 2014-3-6---错误数加1---by余楷
