  						}
  						else
  						{
  							parsetreeroot = NULL;
  						} 
  						/* output($$, 1);*/ 
  					}	
	| stmt_list stmt ';'	{ 		// 2014-3-4---增加对多条指令的识别支持---by余楷		// 2014-3-6---解析错误后置根结点为空---by余楷
	  					printf(">> \n"); 
